{
  "manifest_version": 3,
  "name": "ezLife",
  "version": "1.0",
  "description": "Block URLs dynamically from a popup.",
  "permissions": [
    "storage",
    "declarativeNetRequest"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "48": "icon.png"
  }
}